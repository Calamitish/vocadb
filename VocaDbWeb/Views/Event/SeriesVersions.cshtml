@using VocaDb.Web.Code
@using VocaDb.Web.Models.Shared.Partials.ArchivedEntry
@model VocaDb.Web.Models.Shared.Versions<VocaDb.Model.DataContracts.ReleaseEvents.ReleaseEventSeriesContract>

@{
	ViewBag.Parents = new[] {
		Html.ActionLink(ViewRes.SharedStrings.ReleaseEvents, "Index"),
		Html.ActionLink(Model.Entry.Name, "SeriesDetails", new { id = Model.Entry.Id })
	};
}

<partial name="Partials/ArchivedEntry/_CurrentVersionMessage" model="@(new CurrentVersionMessageViewModel(Model.Entry.Version, Model.Entry.Status))" />

<partial name="Partials/ArchivedEntry/_ArchivedObjectVersions" model="@(new ArchivedObjectVersionsViewModel(Model.ArchivedVersions, id => Url.Action("ViewSeriesVersion", new { id = id })))" />