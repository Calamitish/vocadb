@using Microsoft.AspNetCore.Mvc.Rendering
@using VocaDb.Web.Code
@using VocaDb.Web.Models.Shared.Partials.ArchivedEntry
@model VocaDb.Web.Models.Artist.Versions

@{
	ViewBag.Parents = new[] { 
		Html.ActionLink(ViewRes.SharedStrings.Artists, "Index"), 
		Html.ActionLink(Model.Artist.Name, "Details", new { id = Model.Artist.Id }) 
	};
}

<partial name="Partials/ArchivedEntry/_CurrentVersionMessage" model="@(new CurrentVersionMessageViewModel(Model.Artist.Version, Model.Artist.Status))" />

<partial name="Partials/ArchivedEntry/_ArchivedObjectVersions" model="@(new ArchivedObjectVersionsViewModel(Model.ArchivedVersions, id => Url.Action("ViewVersion", new { id = id })))" />