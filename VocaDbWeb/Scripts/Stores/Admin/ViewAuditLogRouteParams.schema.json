{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"properties": {
		"excludeUsers": {
			"type": "string"
		},
		"filter": {
			"type": "string"
		},
		"group": {
			"enum": [
				"Admin",
				"Limited",
				"Moderator",
				"Nothing",
				"Regular",
				"Trusted"
			],
			"type": "string"
		},
		"onlyNewUsers": {
			"type": "boolean"
		},
		"userName": {
			"type": "string"
		}
	},
	"type": "object"
}
